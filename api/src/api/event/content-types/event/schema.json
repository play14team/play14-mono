{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "start": {
      "type": "datetime",
      "required": true,
      "default": "2023-01-01T17:00:00.000Z"
    },
    "end": {
      "type": "datetime",
      "required": true,
      "default": "2023-01-03T16:30:00.000Z"
    },
    "status": {
      "type": "enumeration",
      "enum": ["Announced", "Open", "Over", "Cancelled"],
      "default": "Announced",
      "required": true
    },
    "defaultImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "contactEmail": {
      "type": "email",
      "default": "team@play14.org",
      "regex": "(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|\"(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21\\x23-\\x5b\\x5d-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])*\")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\\[(?:(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9]))\\.){3}(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9])|[a-z0-9-]*[a-z0-9]:(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21-\\x5a\\x53-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])+)\\])"
    },
    "timetable": {
      "type": "component",
      "repeatable": true,
      "component": "events.timetable",
      "required": false
    },
    "registration": {
      "type": "component",
      "repeatable": false,
      "component": "registration.registration",
      "required": false
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-location.event-location",
      "inversedBy": "events"
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue",
      "inversedBy": "events"
    },
    "hosts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "inversedBy": "hosted"
    },
    "mentors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "inversedBy": "mentored"
    },
    "sponsorships": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "events.sponsorship",
      "required": false
    },
    "media": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "events.media",
      "required": false
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "mappedBy": "attended"
    },
    "timezone": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": false,
      "customField": "global::timezone"
    },
    "tagline": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "finance": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "reporting.finance"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "position": {
      "type": "enumeration",
      "enum": ["Player", "Host", "Mentor", "Founder"],
      "required": true
    },
    "company": {
      "type": "string",
      "required": false
    },
    "tagline": {
      "type": "string"
    },
    "bio": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "website": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "socialNetworks": {
      "type": "component",
      "repeatable": true,
      "component": "contact.social-network"
    },
    "attended": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "players"
    },
    "hosted": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "hosts"
    },
    "mentored": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "mentors"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "player"
    },
    "documented": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::game.game",
      "mappedBy": "documentedBy"
    },
    "proposed": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::game.game",
      "mappedBy": "proposedBy"
    },
    "location": {
      "type": "customField",
      "customField": "global::map-field"
    }
  }
}
